{
  "name": "sp-display-templates",
  "version": "1.0.0",
  "description": "A better way of building SharePoint Display Templates",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:watch cdn",
    "cdn": "http-server CDN -p 7777 -c-1",
    "dev": "SET NODE_ENV=development&& webpack",
    "dev:watch": "SET NODE_ENV=development&& webpack --watch",
    "build": "SET NODE_ENV=production&& webpack",
    "lint": "eslint src/**/*.js",
    "test": "karma start",
    "coverage": "SET NODE_ENV=coverage&&karma start"
  },
  "keywords": [
    "SharePoint",
    "Display",
    "Templates",
    "react",
    "livereload"
  ],
  "author": "Kiran",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "chai-jquery": "^2.0.0",
    "compression-webpack-plugin": "^0.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs": "0.0.2",
    "handlebars-loader": "^1.3.0",
    "http-server": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "jquery": "^3.1.0",
    "karma": "^0.13.22",
    "karma-chai-plugins": "^0.7.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.0.0",
    "react-addons-test-utils": "^15.3.0",
    "webpack": "^1.13.1",
    "webpack-livereload-plugin": "^0.8.1"
  }
